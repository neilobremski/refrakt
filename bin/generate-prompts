#!/usr/bin/env python3
"""Generate Suno prompts from enriched playlist data. Wrapper for lib/generate_prompts.py."""
import os, sys, site, glob
_root = os.path.dirname(os.path.dirname(os.path.abspath(__file__)))
_sp = glob.glob(os.path.join(_root, ".venv/lib/python*/site-packages"))
if _sp: site.addsitedir(_sp[0])
sys.path.insert(0, os.path.join(_root, "lib"))
from generate_prompts import main
main()
